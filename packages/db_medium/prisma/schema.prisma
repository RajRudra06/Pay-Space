generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_MEDIUM")
}

model SanctionsList{
  id              Int               @id @default(autoincrement())

  fullName        String
  DOB             DateTime?
  nationalCountry String
  sanctionCountry String
  sanctionReason  String
  sourceList      SourcesList
}

enum SourcesList{
  OFAC
  UN
  EU
}